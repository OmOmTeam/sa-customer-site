<% include components/header %>

<div style="padding-top: 50px" class="container text-center">
  <h3>Reserve Callback</h3><br>
</div><br>

<form class="form-horizontal" action="#">
 <div class="form-group">
   <label class="control-label col-sm-offset-3 col-sm-2" for="name">Your Name:</label>
   <div class="col-sm-3">
     <input type="input" class="form-control" id="name" placeholder="Your Name">
   </div>
 </div>
 <div class="form-group">
   <label class="control-label col-sm-offset-3 col-sm-2" for="phone">Your Phone:</label>
   <div class="col-sm-3">
     <input type="input" class="form-control" id="phone" placeholder="Your Phone Number">
   </div>
 </div>
 <div class="form-group">
   <div class="col-sm-offset-5 col-sm-1">
     <button type="submit" class="btn btn-success">Reserve</button>
   </div>
 </div>
</form>

<footer style="position: absolute; bottom: 0; width: 100%;" class="container text-center">
  <p>ACME Inc.</p>
  <p>2000 - 2018 </p>
</footer>

</body>
</html>
